<?xml version="1.0" ?>
<odoo>

	<record id="view_move_form_inherit_po_info" model="ir.ui.view">
		<field name="name">account.move.form.inherit.po.info</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_move_form"/>
		<field name="arch" type="xml">
			<xpath expr="//page[@name='other_info']" position="inside">
				<group string="Other Information">
					<group>
						<field name="po_no"/>
						<field name="po_date"/>
						<field name="transport"/>
						<field name="lr_no"/>
						<field name="lr_date"/>
					</group>
				</group>
			</xpath>
		</field>
	</record>

</odoo>
