<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		
		<record id="hr_view_employee_form_inherit" model="ir.ui.view">
			<field name="name">hr.employee.form.inherit</field>
			<field name="model">hr.employee</field>
			<field name="inherit_id" ref="hr.view_employee_form" />
			<field name="arch" type="xml">
				<xpath expr="//notebook" position="inside">
					<page name="Zip/State Distribute">
						<field name="zip_distribute">
							<list editable="bottom">
								<field name="state_id" required="1" />
								<field name="zip"/>
								<field name="state_zip_dict" column_invisible="1" />
								<field name="state_dict" column_invisible="1" />
							</list>
						</field>
					</page>
				</xpath>
			</field>
		</record>
		
	</data>
</odoo>